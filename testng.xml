<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="Academy.Listeners"></listener>

	</listeners>
	<test name="App Testing">
		<classes>
			<class name="Academy.AppTest" />
		</classes>
	</test>

	<test name="Validating Login">
		<classes>
			<class name="Academy.HomePage" />
		</classes>
	</test>

	<test name="Validating Title">
		<classes>
			<class name="Academy.validateTitle" />
		</classes>
	</test>

	<test name="Validating Nav Bar">
		<classes>
			<class name="Academy.validateNavigationBar" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->